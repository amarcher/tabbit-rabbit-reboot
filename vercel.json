{
  "framework": null,
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "functions": {
    "api/aasa.js": {
      "runtime": "@vercel/node@3"
    }
  },
  "routes": [
    {
      "src": "/.well-known/apple-app-site-association",
      "dest": "/api/aasa"
    },
    {
      "src": "/privacy",
      "dest": "/privacy",
      "headers": { "Content-Type": "text/html; charset=utf-8" }
    },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
