{
  "framework": null,
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "routes": [
    {
      "src": "/.well-known/apple-app-site-association",
      "dest": "/api/.well-known/apple-app-site-association.js"
    },
    {
      "src": "/privacy",
      "dest": "/privacy",
      "headers": { "Content-Type": "text/html; charset=utf-8" }
    },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
