{
  "routes": [
    {
      "src": "/.well-known/apple-app-site-association",
      "dest": "/api/aasa"
    },
    {
      "src": "/privacy",
      "dest": "/privacy",
      "headers": { "Content-Type": "text/html; charset=utf-8" }
    },
    {
      "src": "/api/bill-image(.*)",
      "dest": "/api/bill-image.tsx"
    },
    {
      "src": "/api/bill/([^/]+)",
      "dest": "/api/bill/[token].js?token=$1"
    },
    {
      "src": "/bill/(.*)",
      "has": [
        {
          "type": "header",
          "key": "user-agent",
          "value": "(?i).*(facebookexternalhit|Twitterbot|WhatsApp|Slackbot|LinkedInBot|Discordbot|TelegramBot).*"
        }
      ],
      "dest": "/api/bill-og"
    },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}
